    "description": "Simple tester class designed to write BDD-style PHPUnit tests. Allows to express what test intended to check in human-readable format.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Kolodko",
            "email": "deathangel138@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.4"
    },
    "autoload": {
        "psr-4": {
            "DeKey\\Tester\\": "src"
        }
    }
}
