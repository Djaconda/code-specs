{
    "name": "php-kitchen/code-specs",
    "description": "BDD extension for PHPUnit that allows to write tests as a specifications in a human-readable format.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "PHPUnit",
        "BDD",
        "specification",
        "spec",
        "tests",
        "tester",
        "extension",
        "unit tests"
    ],
    "authors": [
        {
            "name": "Dmitry Kolodko",
            "email": "prowwid@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "php-kitchen/code-specs-core": "3.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PHPKitchen\\CodeSpecs\\": "src",
            "Tests\\": "tests"
        }
    }
}
