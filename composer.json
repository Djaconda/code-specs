{
    "name": "php-kitchen/code-specs",
    "description": "BDD extension for PHPUnit that allows to write tests as a specifications in a human-readable format.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/php-kitchen/code-specs",
    "keywords": [
        "PHPUnit",
        "BDD",
        "specification",
        "spec",
        "tests",
        "tester",
        "extension",
        "unit tests"
    ],
    "support": {
        "issues": "https://github.com/php-kitchen/code-specs/issues?state=open",
        "forum": "https://discord.gg/Ez5VZhC",
        "docs": "https://github.com/php-kitchen/code-specs/tree/master/docs",
        "source": "https://github.com/php-kitchen/code-specs"
    },
    "authors": [
        {
            "name": "Dmitry Kolodko",
            "email": "prowwid@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-coveralls/php-coveralls": "^2.5",
        "codeception/codeception": "^4.1",
        "dms/phpunit-arraysubset-asserts": "^v0.3.0",
        "codeception/module-asserts": "^1.3",
        "codeception/phpunit-wrapper": "^9.0",
        "ext-simplexml": "*",
        "rector/rector": "^0.12.5"
    },
    "autoload": {
        "psr-4": {
            "PHPKitchen\\CodeSpecs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
